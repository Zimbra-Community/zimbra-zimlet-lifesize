   <handlerObject>fr_cd21_startmeeting_HandlerObject</handlerObject>
   <includeCSS>fr_cd21_startmeeting.css</includeCSS>
   <zimletPanelItem label="Zimlet Lifesize" icon="cd21_startmeeting-panelIcon">
        <toolTipText>Click droit pour afficher le menu</toolTipText>
        <contextMenu>
            <menuItem label="About" id="STARTMEETING_ABOUT" />
        </contextMenu>
    </zimletPanelItem>
</zimlet>
